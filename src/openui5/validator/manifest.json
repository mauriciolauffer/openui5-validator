{
  "sap.app": {
    "id": "openui5.validator",
    "type": "library",
    "applicationVersion": {
      "version": "${version}"
    },
    "title": "An OpenUI5 library to validate fields."
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    }
  }
}
